{
  "name": "archimydes-platform",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "3.1.1",
    "@material-ui/icons": "3.0.1",
    "@types/googlemaps": "3.30.13",
    "@types/markerclustererplus": "2.1.33",
    "ajv": "5.0.0",
    "animate.css": "3.7.0",
    "axios": "^0.18.0",
    "bootstrap": "^4.1.3",
    "classnames": "2.2.6",
    "connected-react-router": "^4.5.0",
    "draft-js": "^0.10.5",
    "draft-js-import-html": "^1.3.3",
    "draftjs-to-html": "^0.8.4",
    "env-cmd": "^8.0.2",
    "history": "4.7.2",
    "jquery": "^3.3.1",
    "lodash-es": "^4.17.11",
    "moment": "^2.23.0",
    "moment-timezone": "^0.5.23",
    "node-sass": "^4.9.4",
    "node-sass-chokidar": "1.3.3",
    "nouislider": "12.0.0",
    "npm-run-all": "4.1.3",
    "popper.js": "^1.14.4",
    "prop-types": "^15.6.2",
    "query-string": "^6.2.0",
    "react": "16.5.2",
    "react-animate-on-scroll": "2.1.5",
    "react-append-to-body": "^2.0.20",
    "react-bootstrap-wizard": "0.0.5",
    "react-confirm-alert": "^2.0.6",
    "react-datetime": "2.15.0",
    "react-dom": "^16.5.2",
    "react-draft-wysiwyg": "^1.13.2",
    "react-google-maps": "9.4.5",
    "react-image-gallery": "0.8.11",
    "react-moment": "^0.8.4",
    "react-redux": "^5.0.7",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.0.5",
    "react-slick": "0.23.1",
    "react-stepzilla": "^4.8.0",
    "react-stripe-checkout": "^2.6.3",
    "react-stripe-elements": "^2.0.2",
    "react-swipeable-views": "0.13.0",
    "react-tagsinput": "3.19.0",
    "reactstrap": "^6.5.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "env-cmd .env.default npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint:check": "eslint . --ext=js,jsx;  exit 0",
    "lint:fix": "eslint . --ext=js,jsx --fix;  exit 0",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "build:default": "env-cmd .env.default npm run build",
    "build:production": "env-cmd .env.production npm run build",
    "build:staging": "env-cmd .env.staging npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "optionalDependencies": {
    "babel-eslint": "9.0.0",
    "eslint": "5.6.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.1",
    "prettier": "1.15.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
